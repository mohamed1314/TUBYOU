    <h1>ادعوا لإخواننا في فلسطين</h1>
    <button onclick="increaseCounter()">اضغط هنا</button>
    <div id="counter">عدد الأشخاص الذين ضغطوا: 0</div>

    <script>
        // جلب العدد من localStorage إذا كان موجودًا
        let count = localStorage.getItem('prayerCount') ? parseInt(localStorage.getItem('prayerCount')) : 0;
        document.getElementById('counter').textContent = 'عدد الأشخاص الذين ضغطوا: ' + count;

        function increaseCounter() {
            count++;
            document.getElementById('counter').textContent = 'عدد الأشخاص الذين ضغطوا: ' + count;
            // تخزين العدد المحدث في localStorage
            localStorage.setItem('prayerCount', count);
        }
    </script>

